<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:MB"
             xmlns:views="clr-namespace:MB.Views"
             x:Class="MB.Views.MainPage">
    <ContentPage.Content>
        <StackLayout VerticalOptions="Center" Padding="40,20,40,20">
            <Image Source="Icon-60@3x.png" WidthRequest="180" HeightRequest="180"  />
            <Button x:Name="LogoutBtn" Text="Logout" Clicked="Logout_Clicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
